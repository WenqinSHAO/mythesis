doi = {10.1109/TNET.2010.2096232},
file = {:Users/wenqin/Documents/reference base/IEEEACM Transactions on Networking/2011/Augustin, Friedman, Teixeira - Measuring multipath routing in the internet - 2011.pdf:pdf},
