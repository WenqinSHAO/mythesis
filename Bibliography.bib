@article{Turner2009a,
abstract = {Real-world time series are often nonstationary with respect to the parameters of some underlying prediction model (UPM). Furthermore, it is often desirable to adapt the UPM to incoming regime changes as soon as possible, necessitating sequential inference about change point locations. A Bayesian algorithm for online change point detection (BOCPD) has been introduced recently by Adams and MacKay (2007). In this algorithm, uncertainty about the last change point location is updated sequentially, and is integrated out to make online predictions robust to parameter changes. BOCPD requires a set of fixed hyper-parameters which allow the user to fully specify the hazard function for change points and the prior distribution over the parameters of the UPM. In practice, finding the ``right'' hyper-parameters can be quite difficult. We therefore extend BOCPD by introducing hyper-parameter learning, without sacrificing the online nature of the algorithm. Hyper-parameter learning is performed by optimizing the marginal likelihood of the BOCPD model, a closed-form quantity which can be computed sequentially. We illustrate performance on three real-world datasets.},
archivePrefix = {arXiv},
arxivId = {arXiv:0710.3742v1},
author = {Turner, Ryan and Saatci, Yunus and Rasmussen, Carl Edward},
eprint = {arXiv:0710.3742v1},
file = {:Users/wenqin/Documents/reference base/In Practice/2009/Turner, Saatci, Rasmussen - Adaptive Sequential Bayesian Change Point Detection - 2009.pdf:pdf},
journal = {In Practice},
keywords = {learning,statistics {\&} optimisation},
mendeley-groups = {Time series change detection},
pages = {1--4},
title = {{Adaptive Sequential Bayesian Change Point Detection}},
url = {http://eprints.pascal-network.org/archive/00006628/},
year = {2009}
}
@article{Sharkey2014,
abstract = {Changepoints have been extensively analysed in order to identify structural changes in time series$\backslash$ndata, typically when the data are of known parametric form. This report presents an exploration$\backslash$nof methods to detect changepoints in a nonparametric setting, where no assumptions are made$\backslash$nwith regard to the distributional structure of the data, yet must still maintain a specified level$\backslash$nof performance. In particular, the framework of a two-sample hypothesis testing procedure for$\backslash$nsequential testing is developed, in which test statistics based on ranks of observations are adapted$\backslash$nfor changepoint detection. This framework is then extended to consider multiple changepoints$\backslash$nand data streams. The characteristics and performance of the testing procedure are analysed$\backslash$nby comparing the impact of test statistics in a range of scenarios. Under this framework, it$\backslash$nis found that while parametric techniques tend to outperform nonparametric techniques in a$\backslash$nGaussian setting, nonparametric tests are a suitable alternative. In addition, it is found that$\backslash$ntests for arbitrary distributional changes are comparable to tests designed to detect changes in$\backslash$nlocation and scale. Overall, the nonparametric hypothesis testing procedure is found to perform$\backslash$nwell, and represents a logical course of action when performing changepoint analysis on data of$\backslash$nno known distributional form, a common scenario that applies to a wide variety of real-world$\backslash$nprocesses.},
author = {Sharkey, Paul and Killick, Rebecca},
file = {:Users/wenqin/Documents/reference base/Unknown/2014/Sharkey, Killick - Nonparametric Methods for Online Changepoint Detection - 2014.pdf:pdf},
mendeley-groups = {Time series change detection},
title = {{Nonparametric Methods for Online Changepoint Detection}},
year = {2014}
}
