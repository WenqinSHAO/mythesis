file = {:Users/wenqin/Documents/reference base/Journal of the American Statistical Association/2012/Killick, Fearnhead, Eckley - Optimal detection of changepoints with a linear computational cost - 2012.pdf:pdf},
isbn = {0162-1459$\backslash$r1537-274X},
issn = {0162-1459},
journal = {Journal of the American Statistical Association},
keywords = {dynamic programming,pelt,segmentation,structural change},
mendeley-groups = {Time series change detection},
