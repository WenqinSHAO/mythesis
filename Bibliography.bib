eprint = {1611.03895},
file = {:Users/wenqin/Documents/reference base/PAM/2016/Nomikos, Dimitropoulos - TraIXroute Detecting IXPs in traceroute paths - 2016.pdf:pdf},
isbn = {9783319305042},
issn = {16113349},
mendeley-groups = {Measurement and Analysis},
number = {i},
pages = {346--358},
title = {{TraIXroute: Detecting IXPs in traceroute paths}},
volume = {9631},
