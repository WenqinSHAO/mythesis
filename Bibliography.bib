keywords = {average of,for example,highly asymmetric traffic demands,our measurements,scalability,show that the princeton,software-defined networking,traffic engineering,university campus receives an},
mendeley-groups = {TE,BGP/TE (1)},
pages = {1--7},
title = {{Scalable programmable inbound traffic engineering}},
url = {http://dl.acm.org/citation.cfm?doid=2774993.2775063},
year = {2015}
