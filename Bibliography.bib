issn = {0960-3174},
journal = {Statistics and Computing},
keywords = {Activity tracking,CROPS,Nonparametric maximum likelihood,PELT,activity tracking,crops,nonparametric maximum likelihood,pelt},
mendeley-groups = {Time series change detection},
publisher = {Springer US},
title = {{A computationally efficient nonparametric approach for changepoint detection}},
year = {2016}
}

