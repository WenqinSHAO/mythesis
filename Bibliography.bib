@article{Gao2001a,
abstract = {The Internet consists of rapidly increasing number of hosts$\backslash$ninterconnected by constantly evolving networks of links and routers.$\backslash$nInterdomain routing in the Internet is coordinated by the Border Gateway$\backslash$nProtocol (BGP). The BGP allows each autonomous system (AS) to choose its$\backslash$nown administrative policy in selecting routes and propagating$\backslash$nreachability information to others. These routing policies are$\backslash$nconstrained by the contractual commercial agreements between$\backslash$nadministrative domains. For example, an AS sets its policy so that it$\backslash$ndoes not provide transit services between its providers. Such policies$\backslash$nimply that AS relationships are an important aspect of the Internet$\backslash$nstructure. We propose an augmented AS graph representation that$\backslash$nclassifies AS relationships into customer-provider, peering, and sibling$\backslash$nrelationships. We classify the types of routes that can appear in BGP$\backslash$nrouting tables based on the relationships between the ASs in the path$\backslash$nand present heuristic algorithms that infer AS relationships from BGP$\backslash$nrouting tables. The algorithms are tested on publicly available BGP$\backslash$nrouting tables. We verify our inference results with AT{\&}amp;T internal$\backslash$ninformation on its relationship with neighboring ASs. As much as 99.1{\%}$\backslash$nof our inference results are confirmed by the AT{\&}amp;T internal$\backslash$ninformation. We also verify our inferred sibling relationships with the$\backslash$ninformation acquired from the WHOIS lookup service. More than half of$\backslash$nour inferred sibling-to-sibling relationships are confirmed by the WHOIS$\backslash$nlookup service. To the best of our knowledge, there has been no publicly$\backslash$navailable information about AS relationships and this is the first$\backslash$nattempt in understanding and inferring AS relationships in the Internet.$\backslash$nWe show evidence that some routing table entries stem from router$\backslash$nmisconfigurations},
author = {Gao, Lixin},
doi = {10.1109/90.974527},
file = {:Users/wenqin/Documents/reference base/IEEEACM Transactions on Networking/2001/Gao - On inferring autonomous system relationships in the Internet - 2001.pdf:pdf},
