keywords = {,Cartesian coordinate system,Coordinate measuring machines,Data mining,Delay effects,Delay estimation,Delay systems,Economic indicators,IP networks,Internet,Internet coordinate system,PCA,Principal component analysis,Robustness,beacon hosts,delay estimation,delay measurement,measurement techniques,modeling techniques,network distance estimation,network topology,principal component analysis,telecommunication network topology,topological information},
month = jun,
number = {3},
